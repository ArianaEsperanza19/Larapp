{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@now/static-build",
            "config": {
                "buildCommand": "npm install && npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}
